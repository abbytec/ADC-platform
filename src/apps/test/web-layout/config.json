{
	"uiModule": {
		"name": "layout",
		"framework": "react",
		"outputDir": "dist-ui",
		"isHost": true,
		"isRemote": false,
		"uiDependencies": ["web-ui-library", "home"],
		"devPort": 3004,
		"sharedLibs": ["react", "tailwind"],
		"i18n": true,
		"serviceWorker": true,
		"hosting": {
			"hosts": [
				{
					"domain": "local.com",
					"subdomains": ["cloud", "users", "config", "*"]
				}
			]
		}
	},
	"providers": [
		{
			"name": "mongo",
			"version": "latest",
			"language": "typescript",
			"custom": {
				"uri": "mongodb://admin:password@localhost:27018/adc-dashboard?authSource=admin",
				"maxRetries": 5,
				"retryDelay": 5000
			}
		}
	],
	"services": [
		{
			"name": "IdentityManagerService",
			"version": "latest",
			"language": "typescript"
		}
	]
}
