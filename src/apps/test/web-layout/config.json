{
	"uiModule": {
		"name": "layout",
		"framework": "react",
		"outputDir": "dist-ui",
		"standalone": true,
		"devPort": 3004,
		"sharedLibs": ["react"]
	},
	"providers": [
		{
			"name": "mongo",
			"version": "latest",
			"language": "typescript",
			"config": {
				"uri": "mongodb://admin:password@localhost:27018/adc-dashboard?authSource=admin",
				"maxRetries": 5,
				"retryDelay": 5000
			}
		}
	],
	"services": [
		{
			"name": "IdentityManagerService",
			"version": "latest",
			"language": "typescript"
		}
	]
}
